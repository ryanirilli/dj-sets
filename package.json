    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.24",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.example.yourapp",
    "productName": "TheFullSet",
    "files": [
      "electron/**/*",
      "node_modules/**/*",
      "package.json",
      "next.config.js",
      ".next/standalone/**/*",
      ".next/static/**/*",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
